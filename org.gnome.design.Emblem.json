{
    "id": "org.gnome.design.Emblem",
    "runtime": "org.gnome.Platform",
    "runtime-version": "43",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": ["org.freedesktop.Sdk.Extension.rust-stable"],
    "command": "emblem",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "emblem",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/World/design/emblem/uploads/e5211dcfda2e30e117cf3b1719aa9ed1/emblem-1.1.0.tar.xz",
                    "sha256": "d729a312fcc994e0275b4aa9136cda7b4274168ad13a98a60b38c8eadc63e05a"
                }
            ]
        }
    ]
}
