{
    "id": "org.gnome.design.Emblem",
    "runtime": "org.gnome.Platform",
    "runtime-version": "45",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": ["org.freedesktop.Sdk.Extension.rust-stable"],
    "command": "emblem",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "emblem",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/World/design/emblem/uploads/e67aadb1681f298335f4d40f1904a46c/emblem-1.3.0.tar.xz",
                    "sha256": "551ecef3c786db5210be28db1b5801d289c90476ba4608936df8c50a2882041f"
                }
            ]
        }
    ]
}
