{
    "id": "org.gnome.design.Emblem",
    "runtime": "org.gnome.Platform",
    "runtime-version": "44",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": ["org.freedesktop.Sdk.Extension.rust-stable"],
    "command": "emblem",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "emblem",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/World/design/emblem/uploads/b2d7f9ea8d946f5036a4ee770b9313b1/emblem-1.2.0.tar.xz",
                    "sha256": "a847be4e18c426b31ed20001870648711ce9483e53cb5851e91b24a89eb4c045"
                }
            ]
        }
    ]
}
